  import ListProjects from "@/components/ListProjects.astro";
  import { getProjects } from "@/lib/utils";
  import MoreInfo from "@/components/MoreInfo.astro";

  const projects = await getProjects()
---
  <Layout title="Ubicaciones">
    <section>
      <div class="container">
        <div class="mt-10 bg-black/95 py-28 flex flex-col gap-10 items-center justify-center rounded-md">
          <h1 class="text-white font-heading text-7xl">Ubicaci√≥n</h1>
          <ListUbicaciones/>
        </div>
        <div class="pt-10 pb-28">
          <ListProjects projects={projects}/>     
        </div>
      </div>
    </section>
    <MoreInfo/>
  </Layout>